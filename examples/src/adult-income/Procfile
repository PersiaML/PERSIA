# Trick to block the data_loader to prevent process exit before training done.(works for linux and macos)
data_loader: persia-launcher data-loader data_loader.py --replica-index 0 --replica-size 1 && cat 
nn_worker: persia-launcher nn-worker train.py
embedding_worker: persia-launcher embedding-worker --embedding-config config/embedding_config.yml  --global-config config/global_config.yml
embedding_server: persia-launcher embedding-parameter-server --embedding-config config/embedding_config.yml --global-config config/global_config.yml
nats_server: nats-server
